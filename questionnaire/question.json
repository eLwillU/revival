          "url": "http://hl7.org/fhir/StructureDefinition/translation",
          "extension": [
            {
              "url": "lang",
              "valueCode": "fr"
            },
            {
              "url": "translation",
              "valueString": "Combien de fois avez-vous consulté un médecin généraliste ou un médecin de famille en raison des symptômes causés par votre cancer avant de consulter un spécialiste ?"
